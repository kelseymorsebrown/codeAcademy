<div class="newsstand">
  <div class="header">
    <div class="container">
      <%= link_to("/newsstand") do %>
        <h1><span class="dark">n</span>ewstand</h1>
      <% end %>

      <div class="nav">
        <% if current_user %>
          <ul>
            <li><%= current_user.email %></li>
            <li><%= button_to "Log out", newsstand_logout_path, method: "delete", class: 'btn btn-small' %></li>
          </ul>
        <% else %>
          <ul>
            <li><%= link_to "Login", '/newsstand/login' %></a></li>
            <li><%= link_to "Signup", '/newsstand/signup' %></a></li>
          </ul>
        <% end %>
      </div>
    </div>
  </div>
  
  <div class="article">
    <div class="container">
      <p class="date"><%= @article.created_at.strftime('%b %d') %></p>
